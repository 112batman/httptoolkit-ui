  "scripts": {
    "prebuild": "rimraf dist/*",
    "build": "TS_NODE_PROJECT=./automation/tsconfig.json node --max_old_space_size=4096 node_modules/.bin/webpack --config ./automation/webpack.prod.ts",
    "start": "node-dev ./node_modules/.bin/httptoolkit-server start & npm run start:web",
    "start:web": "TS_NODE_PROJECT=./automation/tsconfig.json webpack-dev-server --config ./automation/webpack.dev.ts",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "TS_NODE_PROJECT=./test/tsconfig.json karma start ./test/unit/karma.conf.js",
    "test:unit:dev": "npm run test:unit -- --single-run=false --auto-watch",
    "test:unit:debug": "npm run test:unit -- --single-run=false --browsers Chrome",
    "test:integration": "npm run build && npm run test:integration:run",
    "test:integration:run": "TS_NODE_PROJECT=./test/tsconfig.json TS_NODE_FILES=true mocha --exit -r ts-node/register 'test/integration/**/*.spec.ts'"
  },
